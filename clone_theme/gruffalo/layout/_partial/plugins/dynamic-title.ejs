<% if (theme.title_change) { %>
<script>
(function() {
    var OriginTitile = document.title, titleTime;
    document.addEventListener('visibilitychange', function() {
        if (document.hidden) {
            document.title = '<%- theme.title_change.leave %>';
            clearTimeout(titleTime);
        } else {
            document.title = '<%- theme.title_change.normal %>';
            titleTime = setTimeout(function() {
                document.title = OriginTitile;
            },2000);
        }
    });
})();
</script>
<% } %>
<!-- 全局注入js -->
<script type="text/javascript">
  var twoIcon = document.getElementsByClassName('fade-scale');
  var iconLen = twoIcon.length;
  var pathName = window.location.pathname;
  // console.log(pathName.length)
  if (pathName.split('/').length > 3) {
    for (var i = 0; i < iconLen; i++) {
      twoIcon[i].style.display = "none";
    }
  }
</script>